{
  "name": "react-feedback-hooks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://yevhenchementsov.github.io/react-feedback-hooks/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier . --cache --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "vite": "^4.4.5"
  },
  "lint-staged": {
    "*.{js,jsx}": "yarn lint",
    "*.{js,jsx,css,md,html,json}": "yarn format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  }
}
